{
  "expo": {
    "name": "Play Match",
    "slug": "play-match",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.txt",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.txt",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.txt",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
